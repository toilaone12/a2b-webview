{
  "expo": {
    "name": "a2b-app-2",
    "slug": "a2b-app-2",
    "version": "1.0.0",
    "plugins": [
      "expo-apple-authentication"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bokazem.a2bapp2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bokazem.a2bapp2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "a2b-app-2",
    "extra": {
      "auth": {
        "useProxy": false
      }
    }
  }
}
